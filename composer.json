{
    "name": "bahaso/laravel-passport",
    "description": "Bahaso Passport provides OAuth2 server support to Laravel with additional compatibility to handle custom models.",
    "keywords": ["laravel", "passport", "oauth", "bahaso"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/bahaso/laravel-passport/issues",
        "source": "https://github.com/bahaso/laravel-passport"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Edwin Fadilah",
            "email": "edwinfadilah@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "firebase/php-jwt": "^3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/auth": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/encryption": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "league/oauth2-server": "^8.0",
        "phpseclib/phpseclib": "^3.0",
        "symfony/psr-http-message-bridge": "^2.0",
        "zendframework/zend-diactoros": "~1.0|~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "Bahaso\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bahaso\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        },
        "laravel": {
            "providers": [
                "Bahaso\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
